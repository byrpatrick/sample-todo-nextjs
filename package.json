{
  "name": "zenstack-todo-sample-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate && npm run lint && next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "test-ui:e2e": "playwright test --ui",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset -f",
    "db:browse": "prisma studio",
    "generate": "zenstack generate",
    "vercel-build": "npm run build && npm run db:deploy",
    "package-clean": "npm rm zenstack @zenstackhq/runtime @zenstackhq/server @zenstackhq/swr",
    "up": "npm run package-clean && npm install -D --save-exact zenstack@latest @zenstackhq/swr@latest && npm install --save-exact @zenstackhq/runtime@latest @zenstackhq/server@latest",
    "up-preview": "npm run package-clean && npm install --registry https://preview.registry.zenstack.dev -D zenstack@latest @zenstackhq/swr@latest && npm install --registry https://preview.registry.zenstack.dev @zenstackhq/runtime@latest @zenstackhq/server@latest"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.12",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^5.13.0",
    "@vercel/analytics": "^1.0.1",
    "@zenstackhq/runtime": "2.2.3",
    "@zenstackhq/server": "2.2.3",
    "babel-plugin-superjson-next": "^0.4.5",
    "bcryptjs": "^2.4.3",
    "daisyui": "^4.4.10",
    "moment": "^2.29.4",
    "nanoid": "^3.3.4",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.0.8",
    "superjson": "^1.12.0",
    "swr": "^2.1.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.44.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^14.18.63",
    "@types/react": "^18.2.22",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@zenstackhq/swr": "2.2.3",
    "autoprefixer": "^10.4.12",
    "eslint": "^7.19.0",
    "eslint-config-next": "12.3.1",
    "postcss": "^8.4.16",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.1.8",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "zenstack": "2.2.3"
  }
}
